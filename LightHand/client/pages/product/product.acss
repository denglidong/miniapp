page {
  padding-top: 62%;
  padding-bottom: 4%;
}

.am-tabs-scroll-left{
  opacity: 0;
}

.am-tabs-scroll-right {
  opacity: 0;
}

.nav-title-me {
  margin: 0 auto;
  width: 192rpx;
  color: rgba(0, 0, 0, 1);
  font-size: 48rpx;
  font-family: PingFangSC-regular;
  margin-top: 60rpx;
}

.tab-bg-color {
  background-color: #f5f5f5;
}

.item-mrg {
  margin-top: 10rpx;
  margin-bottom: 20rpx;
}

.product-count {
  font-family: PingFangSC-regular;
  font-size: 24rpx;
  padding-top: 24rpx;
  padding-left: 32rpx;
  padding-bottom: 24rpx;
  padding-right: 32rpx;
}

.item-content {
  width: 658rpx;
  min-height: 296rpx;
  margin: 0 auto;
  border-radius: 8rpx;
  background-color: #fff;
  padding: 24rpx;
  display: flex;
  flex-wrap: wrap;
}

.item-content-header {
  width: 100%;
  border-bottom: solid #f7f7f7 1rpx;
  font-size: 0.24rem;
  height: 50rpx;
  color: rgba(0, 0, 0, 0.7);
}

.item-content-img {
  width: 180rpx;
  height: 180rpx;
  overflow: hidden;
  margin-top: 24rpx;
}

.item-content-right {
  margin: 24rpx;
  width: 404rpx;
  height: 180rpx;
  display: flex;
  flex-wrap: wrap;
  position: relative;
}

.item-content-right-1 {
  width: 404rpx;
  height: 180rpx;
  display: flex;
  flex-wrap: wrap;
  position: relative;
}

.item-content-title {
  width: 100%;
  height: 32rpx;
  color: rgba(102, 102, 102, 100);
  font-size: 24rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-content-productid {
  width: 100%;
  height: 32rpx;
  color: rgba(102, 102, 102, 100);
  font-size: 24rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  align-self: center;
}

.item-content-property {
  width: 100%;
  height: 32rpx;
  color: rgba(102, 102, 102, 100);
  font-size: 24rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  align-self: flex-end;
}

.item-content-downimg {
  position: absolute;
  right: -48rpx;
  top: 52rpx;
}

.item-downing-size {
  width: 48rpx;
  opacity: 0.6;
}

.item-content-footer {
  width: 702rpx;
  height: 120rpx;
  margin: 0 auto;
  background-color: rgba(255, 255, 255, 0.4);
  border-radius: 0 0 8rpx 8rpx;
  display: flex;
  flex-wrap: wrap;
}

.item-footer-fun {
  width: 351rpx;
  display: flex;
}

.item-footer-fun-sty {
  width: 68rpx;
  margin: 0 auto;
  align-self: flex-end;
}

.item-footer-img {
  width: 48rpx;
  opacity: 0.6;
}

.item-footer-align {
  position: relative;
  left: -8rpx;
  text-align: center;
  font-size: 30rpx;
  opacity: 0.6;
}

.item-footer-onload {
  width: 100%;
  text-align: center;
  border-bottom: solid #f7f7f7 1rpx;
  font-size: 0.24rem;
  height: 50rpx;
  color: rgba(0, 0, 0, 0.7);
}

