/*popover组件开始 */

.demo-popover {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 400px;
}

.demo-popover-btn {
  width: 188rpx;
  height: 68rpx;
  background-color: rgb(50, 120, 250);
  border-radius: 20rpx 0 0 20rpx;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
  line-height: 68rpx;
  box-shadow: 0px 4px 6px #707070;
}

.demo-popover-test-btns {
  display: flex;
  justify-content: space-around;
}

.demo-popover-test-btn {
  width: 45%;
}

.am-popover-arrow {
  width: 20rpx;
  height: 20rpx;
  background: white;
  left: 148rpx;
  top: -22rpx;
}

.am-popover-content {
  top: 330rpx;
  right: -140rpx;
}

.am-popover-inner {
  border-radius: 40rpx;
  top: -90rpx;
  left: 0rpx;
}

.am-popover-arrow {
  background-color: rgb(50, 120, 250);
  top: -26rpx;
}

.am-popover-inner {
  background-color: rgb(50, 120, 250);
  border-radius: 10rpx;
  font-size: 24rpx;
}

.am-popover-item {
  color: #fff;
  margin: 0;
}

