<view class='com-selectBox'>
    <view class='com-sContent' onTap='selectToggle'>
        <view class='com-sTxt'>{{nowText}}</view>
        <image src='../../public/img/local/down.png'  class='com-sImg'  animation="{{animationData}}"></image>
    </view>
    <view class='com-sList' a:if="{{selectShow}}">
        <view a:for="{{propArray}}" data-index="{{index}}" a:key='' class='com-sItem' onTap='setText'>{{item.text}}</view>
    </view>
</view>